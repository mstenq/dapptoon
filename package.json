{
    "name": "chat-room",
    "version": "0.0.1",
    "private": true,
    "homepage": "https://dao-xyz.github.io/peerbit-examples",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "start": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
        "build": "tsc && vite build && bun run clean-dist",
        "clean-dist": "node -e \"const fs = require('fs'); const path = require('path'); const dir = path.join('dist', 'node_modules'); if (fs.existsSync(dir)) fs.rmSync(dir, {recursive: true, force: true});\"",
        "preview": "vite preview",
        "deploy": "yarn build && NODE_DEBUG=gh-pages gh-pages -d dist"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.28.0",
        "@peerbit/document": "^9",
        "@peerbit/react": "*",
        "@protobufjs/float": "^1.0.2",
        "@protobufjs/inquire": "^1.1.0",
        "@protobufjs/pool": "^1.1.0",
        "babel-plugin-react-compiler": "^19.1.0-rc.3",
        "peerbit": "^4",
        "protobufjs": "^7.5.4",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-use": "^17.6.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-methods": "^7.27.1",
        "@peerbit/vite": "^1",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^5.0.4",
        "typescript": "^5.6.3",
        "vite": "^7.1.3"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
